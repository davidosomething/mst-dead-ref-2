{
  "name": "calclife",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-beta.0",
    "@material-ui/icons": "^4.0.0-beta.0",
    "@rehooks/document-title": "^1.0.1",
    "bignumber.js": "^8.1.1",
    "clsx": "^1.0.4",
    "currency.js": "^1.2.1",
    "date-fns": "^1.30.1",
    "eslint-plugin-react-perf": "^3.1.0",
    "flat": "^4.1.0",
    "history": "^4.9.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-logger": "^0.7.1",
    "mobx-react-lite": "^1.3.1",
    "mobx-state-tree": "^3.14.0",
    "mobx-utils": "^5.4.0",
    "mst-middlewares": "^3.14.0",
    "nanoid": "^2.0.1",
    "pify": "^4.0.1",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.2",
    "react-dom": "^16.8.6",
    "react-number-format": "^4.0.7",
    "react-router5": "^7.1.0",
    "react-scripts": "3.0.0",
    "react-select": "^2.4.3",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2",
    "router5-plugin-logger": "^7.0.2",
    "router5-transition-path": "^7.0.1",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-unicorn": "^8.0.2",
    "mobx-devtools-mst": "^0.9.21",
    "mobx-formatters": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "npx eslint src",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "lodash",
      "react-perf",
      "unicorn"
    ],
    "extends": [
      "react-app",
      "plugin:lodash/recommended",
      "plugin:react-perf/recommended",
      "plugin:unicorn/recommended"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "ignorePackages"
      ],
      "import/no-default-export": [
        "error"
      ],
      "lodash/import-scope": "off",
      "lodash/path-style": "off",
      "lodash/prefer-constant": "off",
      "lodash/prefer-lodash-method": [
        2,
        {
          "ignoreMethods": [
            "replace"
          ],
          "ignoreObjects": [
            "types"
          ]
        }
      ],
      "react-perf/jsx-no-new-object-as-prop": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-zero-fractions": "off",
      "unicorn/prevent-abbreviations": [
        "error",
        {
          "replacements": {
            "cb": {
              "callback": false
            },
            "e": {
              "error": false,
              "event": false
            },
            "el": {
              "element": false
            },
            "obj": {
              "object": false
            },
            "prev": {
              "previous": false
            },
            "prop": {
              "property": false
            },
            "props": {
              "properties": false
            },
            "ref": {
              "reference": false
            }
          }
        }
      ]
    }
  },
  "eslintIgnore": [
    "src/registerServiceWorker.js"
  ],
  "remarkConfig": {
    "settings": {
      "blockquote-indentation": 4,
      "emphasis": "*",
      "strong": "*",
      "list-item-indent": "tab"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jscpd": {
    "format": "javascript",
    "minTokens": 40,
    "ignore": [
      "**/__tests__/**"
    ],
    "gitignore": true,
    "absolute": true
  }
}
